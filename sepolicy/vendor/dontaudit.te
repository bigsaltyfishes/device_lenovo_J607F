# Apps are no longer allowed open access to /dev/ashmem, unless they
# target API level < Q.
dontaudit untrusted_app ashmem_device:chr_file open;

dontaudit {
  hal_audio_default
  hal_graphics_composer_default
  hal_sensors_default
  rild
  vendor_adpl
  vendor_cnd
  vendor_dpmd
  vendor_hal_imsrtp
  vendor_hal_rcsservice
  vendor_ims
  vendor_netmgrd
  vendor_qti
  vendor_sensors_qti
} vendor_diag_device:chr_file rw_file_perms;

dontaudit vendor_hal_capabilityconfigstore_qti_default vendor_cap_configstore_dbg_prop:file r_file_perms;

dontaudit surfaceflinger firmware_file:dir search;
dontaudit surfaceflinger vendor_file:file read;
dontaudit surfaceflinger kernel:system module_request;
dontaudit surfaceflinger sysfs_msm_subsys:dir search;
dontaudit surfaceflinger sysfs_msm_subsys:file r_file_perms;

dontaudit system_server self:capability sys_module;